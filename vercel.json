{
  "rewrites": [
    { "source": "/blog", "destination": "/blog" },
    { "source": "/blog/page/:page", "destination": "/blog/page/:page" },
    { "source": "/blog/:slug", "destination": "/blog/:slug" }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
} 